{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "preserveSymlinks": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6",
    "types": ["react", "react-native"],
    "baseUrl": "src/js"
  },
  "include": [
    "src/js/*"
  ]
}
